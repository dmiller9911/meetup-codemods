{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "version": "16.0"
    }
  }
}
